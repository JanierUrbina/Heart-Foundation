@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<br />
<br />
<br />
<br />
    <div class="container">
    <h2>Lista de Solicitudes</h2>
    <div class="table-responsive">
        <table id="miTablaS" class="table table-striped table-bordered table-hover dataTables-List" style="table-layout:fixed;">
            <thead>
                <tr class="table table-bordered">
                     <th class="table-success">
                         Solicitante
                     </th>
                     <th class="table-success">
                         Estado
                     </th>
                     <th class="table-success">
                         Fecha Creación
                     </th>
                     <th class="table-success">
                         Acciones
                     </th>
            </tr>
        </thead>
    </table>
    </div>

    @* imagen *@
    <div class="modal" id="modalimages" tabindex="-1" role="dialog">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">

                    <button type="button" class="btn btn-danger close" data-dismiss="modal" aria-label="Close" onclick="closemodal(this);">
                        <span aria-hidden="true">&times;</span>
                    </button>

                </div>
                <div class="modal-body">
                    <div id="changerid" style="display:none;">
                        <center>
                            <button class="btn btn-info" id="prev" onclick="changeref(-1)" data-indexp="0" disabled>❮</button>
                            <button class="btn btn-info" id="next" onclick="changeref(1)">❯</button>
                        </center>
                    </div>
                    <br />
                    <b id="nota"></b>
                    <img id="miImagen" class="img-thumbnail" width="400" height="50">
                </div>

            </div>
        </div>
    </div>

    <div id="modalGrl" class="modal">
        <div class="modal-dialog" role="document">
            <div id="modalcontentGrl">
            </div>
        </div>
    </div>

</div>



<script src="~/js/datatables/tablas/global.js"></script>
<script src="~/js/datatables/tablas/solicitudesrepresentantes.js"></script>